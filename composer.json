{
    "name": "bek/flarum-ext-trbeta",
    "type": "flarum-extension",
    "description": "Turkish language pack for Flarum beta 5.",
    "keywords": ["flarum", "locale"],
    "license": "MIT",
    "minimum-stability": "beta",
    "require": {
        "flarum/core": "^0.1.0-beta.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Turkish",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "tr",
            "title": "Turkish"
        }
    }
}
