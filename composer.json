{
    "name": "bek/flarum-ext-trbeta",
    "type": "flarum-extension",
    "description": "Turkish language pack for Flarum.",
    "homepage": "https://github.com/bekirkarul/flarum-ext-trbeta",
    "keywords": ["locale"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bekir Karul",
        }
    ],
    "support": {
        "issues": "https://github.com/bekirkarul/flarum-ext-trbeta/issues",
        "source": "https://github.com/bekirkarul/flarum-ext-trbeta",
    },
    "require": {
        "flarum/core": "^0.1.0-beta.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Turkish",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#fff",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "tr",
            "title": "Turkish"
        }
    }
}
